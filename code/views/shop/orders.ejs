<%- include('../includes/head.ejs') %>
</head>

<body>
<%- include('../includes/navigation.ejs') %>
<main>
    <% if (orders.length <= 0){ %>
        <h1>Nothing here!</h1>
    <% } else{ %>
        <ul>
            <% for (order of orders){ %>
                <li>
                    <h1># <%= order.id %></h1>
                    <ul>
                        <% for (product of order.products){ %>
                            <li><%= product.title %> (<%= product.orderItem.quantity %>)</li>
                        <% } %>
                    </ul>
                </li>
            <% } %>
        </ul>
    <% } %>
</main>
<%- include('../includes/end.ejs') %>